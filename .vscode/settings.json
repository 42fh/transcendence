{
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": true,
  "editor.rulers": [120],
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.wordBasedSuggestions": "off",
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    }
  },
  "black-formatter.importStrategy": "useBundled",
  "black-formatter.path": [],
  "black-formatter.args": ["--line-length", "120"],
  "flake8.enabled": true,
  "flake8.args": ["--max-line-length=120"],
  "flake8.importStrategy": "useBundled",
  "flake8.path": [],
  "pylint.enabled": true,
  "pylint.args": ["--disable=E1101"],
  "pylint.importStrategy": "useBundled",
  "pylint.path": [],
  "editor.codeActionsOnSave": {
    "source.fixAll.pylint": "explicit",
    "source.fixAll.flake8": "explicit"
  },
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportGeneralTypeIssues": "none",
    "reportMissingImports": "none",
    "reportMissingTypeStubs": "none",
    "reportMissingModuleSource": "none",
    "reportOptionalMemberAccess": "none",
    "reportUnknownMemberType": "none",
    "reportAttributeAccessIssue": "none"
  },
  "python.globalModuleInstallation": true,
  "python.analysis.autoFormatStrings": false,
  "python.analysis.autoImportCompletions": false,
  "python.analysis.autoIndent": true,
  "python.analysis.autoSearchPaths": true,
  "python.languageServer": "Pylance",
  "makefile.configureOnOpen": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#073609",
    "titleBar.activeBackground": "#0A4B0D",
    "titleBar.activeForeground": "#F0FDF0"
  }
}
